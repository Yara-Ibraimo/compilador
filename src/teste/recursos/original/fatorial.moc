// Ficheiro sem erros sobre a gramatica MOCC.g4

int fact(int n) {
    if (n == 0) {
        return 1;
    } else {
        return n * fact(n - 1);
    }
}

void main(void) {
    int num;
    int result;
    
    num = read();
    result = fact(num);
    write(result);
}
