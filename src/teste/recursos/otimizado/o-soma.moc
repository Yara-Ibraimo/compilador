// Ficheiro otimizado sem erros sobre a gramatica MOCC.g4
// Soma dos primeiros N inteiros

void main(void)
{
    registerint i, n, total;

    writes("Introduza um número inteiro: ");
    n = read();

    total = 0;
    for (i = 1; i <= n; i = i + 1) {
        total = total + i;
    }

    write(total);
}
